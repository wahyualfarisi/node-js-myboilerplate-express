    <!-- Material Dashboard DEMO methods, don't include it in your project! -->
    <script>
      $(document).ready(function() {
        var load_content = function(href) {
          $.get(`/${href}`, content => {
            $("#root").html(content);
          });
        };

        $(window).on("hashchange", () => {
          href = location.hash.substr(2);
          load_content(href);
        });

        if (location.hash) {
          href = location.hash.substr(2);
          load_content(href);
        } else {
          location.hash = "#/dashboard";
        }
      });
    </script>
